<!DOCTYPE html>
<html>
  <head>
    <title>PliosaurusApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <nav id="main-nav">
    <% if( logged_in?  )  %>
      <div><%= link_to 'Log Out', session_url,method: :delete  %></div>
     <% else %>
       <div>  <%= link_to 'Sign Up', new_user_url %>  </div>
       <div><%= link_to 'Sign In', new_session_url %>  </div>

    <% end %>
    
    </nav>
    <%= yield %>
  </body>
</html>
